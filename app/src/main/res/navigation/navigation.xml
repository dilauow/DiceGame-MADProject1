<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/titleFragment">
    <fragment
        android:id="@+id/titleFragment"
        android:name="com.example.dicegame.TitleFragment"
        android:label="fragment_title"
        tools:layout="@layout/fragment_title" >
        <action
            android:id="@+id/action_titleFragment_to_gamePlayFragement5"
            app:destination="@id/gamePlayFragement5" />
    </fragment>
    <fragment
        android:id="@+id/gamePlayFragement5"
        android:name="com.example.dicegame.gamePlayFragement"
        android:label="fragment_game_play_fragement"
        tools:layout="@layout/fragment_game_play_fragement" >
        <action
            android:id="@+id/action_gamePlayFragement5_to_youLost"
            app:destination="@id/youLost" />
        <action
            android:id="@+id/action_gamePlayFragement5_to_youwon"
            app:destination="@id/youwon" />
    </fragment>
    <fragment
        android:id="@+id/youLost"
        android:name="com.example.dicegame.YouLost"
        android:label="fragment_you_lost"
        tools:layout="@layout/fragment_you_lost" >
        <action
            android:id="@+id/action_youLost_to_titleFragment"
            app:destination="@id/titleFragment"
            app:popUpTo="@id/titleFragment" />
    </fragment>
    <fragment
        android:id="@+id/youwon"
        android:name="com.example.dicegame.youwon"
        android:label="fragment_youwon"
        tools:layout="@layout/fragment_youwon" >
        <action
            android:id="@+id/action_youwon_to_titleFragment"
            app:destination="@id/titleFragment"
            app:popUpTo="@id/titleFragment"
            app:popUpToInclusive="false" />
    </fragment>
</navigation>